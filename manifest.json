{
  "name": "GitHub Label Copier",
  "description": "The easiest way to copy GitHub labels between repositories.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/github-label-copier.png",
      "32": "/images/github-label-copier.png",
      "48": "/images/github-label-copier.png",
      "128": "/images/github-label-copier.png"
    }
  }
}
